{
  "name": "@akashic/actions-release",
  "version": "1.0.0",
  "description": "action to relase akashic oss",
  "private": true,
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:ts && npm run bundle && npm run format",
    "build:ts": "tsc -p .",
    "bundle": "ncc build lib/index.js -o dist -m",
    "test": "npm run test:jest && npm run lint",
    "test:jest": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\" --config prettier.config.json",
    "lint": "eslint -c .eslintrc \"src/**/*.ts\" --fix"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@jsdevtools/npm-publish": "^1.4.3"
  },
  "devDependencies": {
    "@akashic/eslint-config": "^0.1.2",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
